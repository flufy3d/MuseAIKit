{
  "name": "museaikit",
  "version": "1.2.0",
  "description": "A powerful music-focused AI toolkit",
  "keywords": [
    "music",
    "ai",
    "machine-learning",
    "tensorflow",
    "midi",
    "music-generation",
    "music-analysis",
    "music-vae",
    "music-rnn"
  ],
  "main": "es5/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "jsdelivr": "dist/museaikit.js",
  "unpkg": "dist/museaikit.js",
  "dependencies": {
    "@tensorflow/tfjs": "^2.7.0",
    "@tensorflow/tfjs-backend-webgl": "^2.7.0",
    "@tonejs/midi": "^2.0.15",
    "fft.js": "^4.0.3",
    "jianpurender": "^1.2.0",
    "ndarray-resample": "^1.0.1",
    "path-browserify": "^1.0.1",
    "protobufjs": "^6.8.6",
    "staffrender": "^0.2.1",
    "tonal": "^2.0.0",
    "tone": "^14.7.58"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/d3": "^5.7.0",
    "@types/file-saver": "^1.3.0",
    "@types/ndarray": "^1.0.6",
    "@types/node": "^18.19.86",
    "@types/node-fetch": "^2.5.7",
    "@types/tape": "^4.2.32",
    "@types/webmidi": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "audio-recorder-polyfill": "^0.1.2",
    "browserify": "^14.4.0",
    "clang-format": "^1.2.3",
    "clone": "^1.0.4",
    "cpx": "^1.5.0",
    "d3": "^5.8.0",
    "eslint": "^9.24.0",
    "file-saver": "^1.3.8",
    "file-saver-typescript": "^1.0.1",
    "http-server": "^0.11.1",
    "in-publish": "^2.0.0",
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.0",
    "rimraf": "^6.0.1",
    "tape": "^4.9.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.4.2",
    "ts-node": "^5.0.1",
    "tsify": "^3.0.4",
    "typedoc": "^0.16.10",
    "typescript": "^4.9.5",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "test-and-build": "yarn lint && yarn test && yarn build && yarn test-node-build",
    "prepublish": "in-publish && yarn test-and-build || not-in-publish",
    "build:es5": "webpack --config ./webpack/es5.lib.config.ts",
    "build:es6": "webpack --config ./webpack/es6.config.ts && cpx \"src/protobuf/proto.*\" \"es6/protobuf\"",
    "build:esm": "tsc --build tsconfig.esm.json && cpx \"src/protobuf/proto.*\" \"esm/protobuf\"",
    "build:node": "webpack --config webpack/node.config.ts && cpx \"src/protobuf/proto.*\" \"node/protobuf\"",
    "build:clean": "rimraf es5 es6 esm node dist",
    "build": "npm run build:clean && npm run build:es5 && npm run build:node && npm run build:es6 && npm run build:esm",
    "lint": "eslint src/**/*.ts",
    "test": "ts-node node_modules/tape/bin/tape src/**/*_test.ts",
    "test-node-build": "ts-node node_modules/tape/bin/tape build_tests/node.ts",
    "build-demos": "webpack --config ./webpack/es5.demo.config.ts",
    "run-demos": "webpack-dev-server --config ./webpack/es5.demo.config.ts",
    "serve-demos": "http-server demos/",
    "serve-dist": "http-server dist/",
    "proto": "sh ./scripts/compile-proto.sh",
    "docs": "sh ./scripts/generate-docs.sh"
  },
  "author": "flufy3d",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/flufy3d/MuseAIKit.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
