{
    "version": "2.0.0",
    "tasks": [
      {
        "command": "yarn",
        "label": "lint",
        "type": "shell",
        "args": [
          "lint"
        ],
        "problemMatcher": {
          "base": "$eslint-stylish",
          "owner": "eslint",
          "fileLocation": "absolute"
        }
      },
      {
        "command": "yarn",
        "label": "build",
        "type": "shell",
        "args": ["build", "--pretty", "false"],
        "problemMatcher": [
          "$tsc"
        ]
      },
      {
        "command": "yarn",
        "label": "build-clean",
        "type": "shell",
        "args": ["build:clean"],
        "problemMatcher": []
      }
    ]
  }
